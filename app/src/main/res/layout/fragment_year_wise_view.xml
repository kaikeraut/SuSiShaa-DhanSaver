<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.YearWiseViewFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/buttonPrev"
                style="@style/Widget.Material3.Button.OutlinedButton"
                android:textColor="@color/buttonTextColor"
                app:strokeColor="@color/buttonStrokeColor"
                android:textSize="14sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:layout_marginTop="2dp"
                android:layout_marginEnd="5dp"
                android:layout_marginBottom="20dp"
                android:layout_weight="0"
                android:text="Prev" />

            <Button
                android:id="@+id/buttonDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:layout_marginTop="2dp"
                android:layout_marginEnd="5dp"
                android:layout_marginBottom="20dp"
                android:layout_weight="10"
                android:text="May, 2024" />

            <Button
                android:id="@+id/buttonNext"
                style="@style/Widget.Material3.Button.OutlinedButton"
                android:textColor="@color/buttonTextColor"
                app:strokeColor="@color/buttonStrokeColor"
                android:textSize="14sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:layout_marginTop="2dp"
                android:layout_marginEnd="5dp"
                android:layout_marginBottom="20dp"
                android:layout_weight="0"
                android:text="Next" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textViewMonth1"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_margin="1dp"
                android:layout_weight="1"

                android:text="TextView" />

            <TextView
                android:id="@+id/textViewMonth2"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_margin="1dp"
                android:layout_weight="1"

                android:text="TextView" />

            <TextView
                android:id="@+id/textViewMonth3"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_margin="1dp"
                android:layout_weight="1"

                android:text="TextView" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textViewMonth4"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_margin="1dp"
                android:layout_weight="1"

                android:text="TextView" />

            <TextView
                android:id="@+id/textViewMonth5"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_margin="1dp"
                android:layout_weight="1"

                android:text="TextView" />

            <TextView
                android:id="@+id/textViewMonth6"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_margin="1dp"
                android:layout_weight="1"

                android:text="TextView" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textViewMonth7"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_margin="1dp"
                android:layout_weight="1"

                android:text="TextView" />

            <TextView
                android:id="@+id/textViewMonth8"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_margin="1dp"
                android:layout_weight="1"

                android:text="TextView" />

            <TextView
                android:id="@+id/textViewMonth9"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_margin="1dp"
                android:layout_weight="1"

                android:text="TextView" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textViewMonth10"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_margin="1dp"
                android:layout_weight="1"

                android:text="TextView" />

            <TextView
                android:id="@+id/textViewMonth11"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_margin="1dp"
                android:layout_weight="1"

                android:text="TextView" />

            <TextView
                android:id="@+id/textViewMonth12"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_margin="1dp"
                android:layout_weight="1"

                android:text="TextView" />

        </LinearLayout>


    </LinearLayout>


</FrameLayout>